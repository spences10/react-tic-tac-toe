{
  "name": "react-tic-tac-toe",
  "version": "1.1.299",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "linaria -o public/styles src/**/*.js && toast incremental",
    "prepare": "breadbox --dest public/web_modules",
    "format": "pretty-quick",
    "serve": "npx serve public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "4.3.0",
    "prettier": "2.1.1",
    "pretty-quick": "3.0.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "printWidth": 70,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "snowpack": {
    "installOptions": {
      "alias": {
        "react": "preact/compat"
      }
    },
    "knownEntrypoints": [
      "preact",
      "preact/hooks",
      "preact/compat",
      "react-helmet"
    ]
  }
}
